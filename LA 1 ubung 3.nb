Cell[TextData[{
 "Also ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"||", 
     SubscriptBox["v", "4"], "||"}], "=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["a", "2"], "+", 
      SuperscriptBox["b", "2"], "+", 
      SuperscriptBox["c", "2"]}], "=", 
     RowBox[{
      RowBox[{"2", " ", 
       SuperscriptBox["a", "2"]}], "=", "1"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "7c439897-419d-4026-85de-692f8f1dc53c"],
 " and thus ",
 Cell[BoxData[
  FormBox[
   RowBox[{"a", "=", 
    FractionBox["1", 
     SqrtBox["2"]]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
