Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["\[CapitalPhi]", "\[ScriptCapitalA]"], "(", 
    RowBox[{
     RowBox[{"\[Alpha]", " ", 
      RowBox[{"(", GridBox[{
         {
          SubscriptBox["a", "0"]},
         {
          SubscriptBox["a", "1"]},
         {
          SubscriptBox["a", "2"]}
        }], ")"}]}], "+", 
     RowBox[{"(", GridBox[{
        {
         SubscriptBox["b", "0"]},
        {
         SubscriptBox["b", "1"]},
        {
         SubscriptBox["b", "2"]}
       }], ")"}]}], ")"}], "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["\[CapitalPhi]", "\[ScriptCapitalA]"], "(", " ", 
     RowBox[{"(", GridBox[{
        {
         RowBox[{
          RowBox[{"\[Alpha]", " ", 
           SubscriptBox["a", "0"]}], "+", 
          SubscriptBox["b", "0"]}]},
        {
         RowBox[{
          RowBox[{"\[Alpha]", " ", 
           SubscriptBox["a", "1"]}], "+", 
          SubscriptBox["b", "1"]}]},
        {
         RowBox[{
          RowBox[{"\[Alpha]", " ", 
           SubscriptBox["a", "2"]}], "+", 
          SubscriptBox["b", "2"]}]}
       }], ")"}], ")"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{"\[Alpha]", " ", 
         SubscriptBox["a", "0"]}], "+", 
        SubscriptBox["b", "0"]}], ")"}], "+", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"\[Alpha]", " ", 
          SubscriptBox["a", "1"]}], "+", 
         SubscriptBox["b", "1"]}], ")"}], "t"}], "+", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"\[Alpha]", " ", 
          SubscriptBox["a", "2"]}], "+", 
         SubscriptBox["b", "2"]}], ")"}], 
       SuperscriptBox["t", "2"]}]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"\[Alpha]", "(", 
        RowBox[{
         SubscriptBox["a", "0"], "+", 
         RowBox[{
          SubscriptBox["a", "1"], "t"}], "+", 
         RowBox[{
          SubscriptBox["a", "2"], 
