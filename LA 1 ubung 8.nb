Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"D", " ", "\[EmptySmallCircle]", " ", "F"}], "-", 
      RowBox[{"F", " ", "\[EmptySmallCircle]", " ", "D"}]}], ")"}], 
    RowBox[{"(", 
     RowBox[{"p", "(", "t", ")"}], ")"}]}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"D", " ", 
      RowBox[{"(", 
       RowBox[{"F", "(", 
        RowBox[{"p", "(", "t", ")"}], ")"}], ")"}]}], "-", 
     RowBox[{"F", "(", 
      RowBox[{"D", "(", 
       RowBox[{"p", "(", "t", ")"}], ")"}], ")"}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"D", "(", 
       RowBox[{"t", " ", 
        RowBox[{"p", "'"}], 
        RowBox[{"(", "t", ")"}]}], ")"}], "-", 
      RowBox[{"F", "(", 
       RowBox[{
        RowBox[{"p", "'"}], 
        RowBox[{"(", "t", ")"}]}], ")"}]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"t", " ", 
          RowBox[{"p", "'"}], 
          RowBox[{"(", "t", ")"}]}], ")"}], "'"}], "-", 
       RowBox[{"t", " ", 
        RowBox[{"p", "''"}], 
        RowBox[{"(", "t", ")"}]}]}], "=", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"p", "'"}], 
         RowBox[{"(", "t", ")"}]}], "+", 
        RowBox[{"t", " ", 
         RowBox[{"p", "''"}], 
         RowBox[{"(", "t", ")"}]}], "-", 
        RowBox[{"t", " ", 
         RowBox[{"p", "''"}], 
         RowBox[{"(", "t", ")"}]}]}], "=", 
       RowBox[{
        RowBox[{
         RowBox[{"p", "'"}], 
         RowBox[{"(", "t", ")"}]}], "=", 
        RowBox[{"D", "(", 
         RowBox[{"p", "(", "t", ")"}], ")"}]}]}]}]}]}]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"d553571d-9c32-499a-95c7-4012f6a24568"]], \
