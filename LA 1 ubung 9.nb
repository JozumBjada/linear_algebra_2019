Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"det", " ", 
    SubscriptBox["A", "0"]}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      SuperscriptBox["\[Lambda]", 
       RowBox[{"n", "-", "2"}]], " ", 
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["\[Lambda]", "2"], "+", 
        RowBox[{"\[Lambda]", " ", 
         SubscriptBox["a", 
          RowBox[{"n", "-", "1"}]]}], "+", 
        SubscriptBox["a", 
         RowBox[{"n", "-", "2"}]]}], ")"}]}], "+", 
     RowBox[{
      SuperscriptBox["\[Lambda]", 
       RowBox[{"n", "-", "3"}]], 
      SubscriptBox["a", 
       RowBox[{"n", "-", "3"}]]}], "+", "\[Ellipsis]", "+", 
     RowBox[{
      SuperscriptBox["\[Lambda]", "2"], 
      SubscriptBox["a", "2"]}], "+", 
     RowBox[{"\[Lambda]", " ", 
      SubscriptBox["a", "1"]}], "+", 
     SubscriptBox["a", "0"]}], "=", 
    RowBox[{
     SuperscriptBox["\[Lambda]", "n"], " ", "+", 
     RowBox[{
      SuperscriptBox["\[Lambda]", 
       RowBox[{"n", "-", "1"}]], " ", 
      SubscriptBox["a", 
       RowBox[{"n", "-", "1"}]]}], "+", 
     RowBox[{
      SuperscriptBox["\[Lambda]", 
       RowBox[{"n", "-", "2"}]], " ", 
      SubscriptBox["a", 
       RowBox[{"n", "-", "2"}]]}], "+", 
     RowBox[{
      SuperscriptBox["\[Lambda]", 
       RowBox[{"n", "-", "2"}]], 
      SubscriptBox["a", 
       RowBox[{"n", "-", "3"}]]}], "+", "\[Ellipsis]", "+", 
     RowBox[{
      SuperscriptBox["\[Lambda]", "2"], 
      SubscriptBox["a", "2"]}], "+", 
     RowBox[{"\[Lambda]", " ", 
      SubscriptBox["a", "1"]}], "+", 
     SubscriptBox["a", "0"]}]}]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"47e53314-3d06-4cf7-b04b-7b538ddcac5b"]], \
