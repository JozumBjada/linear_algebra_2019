Cell[TextData[{
 "This formula allows us to calculate ",
 Cell[BoxData[
  FormBox[
   RowBox[{"det", " ", "A"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "9f574405-d628-42c8-a0a8-3ed9a642e4ca"],
 " in a recursive manner (matrices ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["A", "2"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "564d0ca5-af51-4da9-926f-7f9df38f096a"],
 ", ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["A", "3"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "a25b5783-2438-424b-9ec0-56d0cb87f316"],
 " etc. are defined analogously to ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["A", "0"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "8fc32db2-71a1-46d4-9556-1070ed580da2"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["A", "1"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "4d01d0ae-cc2c-4091-9ce4-3fa14a78bbd7"],
 "):"
}], "Text",
 CellChangeTimes->{{3.7840117097224884`*^9, 3.784011739616485*^9}, {
  3.784011799889264*^9, 3.7840118255975*^9}},
 FontSize->36,ExpressionUUID->"fd709ed9-aa32-41cd-931b-63b86132ca4c"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"det", " ", 
     SubscriptBox["A", "0"]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"\[Lambda]", " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"\[Lambda]", " ", 
          RowBox[{"det", "(", 
           SubscriptBox["A", "2"], ")"}]}], "+", 
         SubscriptBox["a", "1"]}], ")"}]}], "+", 
      SubscriptBox["a", "0"]}], "=", 
     RowBox[{
      RowBox[{"\[Lambda]", " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"\[Lambda]", " ", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"\[Lambda]", " ", 
             RowBox[{"det", "(", 
              SubscriptBox["A", "3"], ")"}]}], "+", 
            SubscriptBox["a", "2"]}], ")"}]}], "+", 
         SubscriptBox["a", "1"]}], ")"}]}], "+", 
      SubscriptBox["a", "0"]}]}]}], TraditionalForm]],ExpressionUUID->
  "d46106ca-ad19-4ce0-a1a4-217cce1d2c73"],
 "\nand so on. In order words:"
}], "Text",
 CellChangeTimes->{{3.7840117435559483`*^9, 3.784011837532575*^9}, {
  3.784012034118846*^9, 3.7840120529155664`*^9}},
 FontSize->36,ExpressionUUID->"fa25b453-1957-481a-ab07-9a2e7418e28a"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"det", " ", 
    SubscriptBox["A", "0"]}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      SuperscriptBox["\[Lambda]", "2"], " ", 
      RowBox[{"det", "(", 
       SubscriptBox["A", "2"], ")"}]}], "+", 
     RowBox[{"\[Lambda]", " ", 
      SubscriptBox["a", "1"]}], "+", 
     SubscriptBox["a", "0"]}], "=", 
    RowBox[{
     RowBox[{
      SuperscriptBox["\[Lambda]", "3"], " ", 
      RowBox[{"det", "(", 
       SubscriptBox["A", "3"], ")"}]}], "+", 
     RowBox[{
      SuperscriptBox["\[Lambda]", "2"], 
      SubscriptBox["a", "2"]}], "+", 
     RowBox[{"\[Lambda]", " ", 
      SubscriptBox["a", "1"]}], "+", 
     SubscriptBox["a", "0"]}]}]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"e89cfc7e-1ec6-42f1-ad0b-4080273de881"]], \
