Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"det", " ", 
    SubscriptBox["A", "0"]}], "=", 
   RowBox[{
    RowBox[{
     SuperscriptBox["\[Lambda]", 
      RowBox[{"n", "-", "2"}]], " ", 
     RowBox[{"det", "(", 
      SubscriptBox["A", 
       RowBox[{"n", "-", "2"}]], ")"}]}], "+", 
    RowBox[{
     SuperscriptBox["\[Lambda]", 
      RowBox[{"n", "-", "3"}]], 
     SubscriptBox["a", 
      RowBox[{"n", "-", "3"}]]}], "+", "\[Ellipsis]", "+", 
    RowBox[{
     SuperscriptBox["\[Lambda]", "2"], 
     SubscriptBox["a", "2"]}], "+", 
    RowBox[{"\[Lambda]", " ", 
     SubscriptBox["a", "1"]}], "+", 
    SubscriptBox["a", "0"]}]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"a8c7471d-4a49-445d-9205-5c48bfa76931"]], \
"Text",
