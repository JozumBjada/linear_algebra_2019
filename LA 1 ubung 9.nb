 FontSize->36,ExpressionUUID->"27bf9b85-af34-4691-baf8-64d511dfbc01"],

Cell[TextData[{
 "As the last part, let us explicitly calculate ",
 Cell[BoxData[
  FormBox[
   RowBox[{"det", "(", 
    SubscriptBox["A", 
     RowBox[{"n", "-", "2"}]], ")"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "031ad89a-46e6-4d81-b842-ca688b39b748"],
 ":"
}], "Text",
 CellChangeTimes->{{3.7840119350537148`*^9, 3.784011945998476*^9}},
 FontSize->36,ExpressionUUID->"78ddc473-98aa-4aa8-87b8-b2897b1d9cbf"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"det", "(", 
    SubscriptBox["A", 
     RowBox[{"n", "-", "2"}]], ")"}], "=", 
   RowBox[{
    RowBox[{"det", "(", 
     RowBox[{"(", GridBox[{
        {"\[Lambda]", 
         SubscriptBox["a", 
          RowBox[{"n", "-", "2"}]]},
        {
         RowBox[{"-", "1"}], 
         RowBox[{"\[Lambda]", "+", 
          SubscriptBox["a", 
           RowBox[{"n", "-", "1"}]]}]}
       }], ")"}], ")"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"\[Lambda]", "(", 
       RowBox[{"\[Lambda]", "+", 
        SubscriptBox["a", 
         RowBox[{"n", "-", "1"}]]}], ")"}], "-", 
      RowBox[{
       SubscriptBox["a", 
        RowBox[{"n", "-", "2"}]], "(", 
       RowBox[{"-", "1"}], ")"}]}], "=", 
     RowBox[{
      SuperscriptBox["\[Lambda]", "2"], "+", 
      RowBox[{"\[Lambda]", " ", 
       SubscriptBox["a", 
        RowBox[{"n", "-", "1"}]]}], "+", 
      SubscriptBox["a", 
       RowBox[{"n", "-", "2"}]]}]}]}]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"ba14f736-009e-4a47-90be-c7136f4fdfef"]], \
