Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"det", " ", 
    SubscriptBox["A", "0"]}], "=", 
   RowBox[{
    RowBox[{"\[Lambda]", " ", 
     RowBox[{"det", "(", 
      SubscriptBox["A", "1"], ")"}]}], "+", 
    SubscriptBox["a", "0"]}]}], 
  TraditionalForm]],ExpressionUUID->"9837fe87-6fe6-4d53-8077-0b59c67a5187"]], \
