Cell[CellGroupData[{

Cell["Useful formulas", "Subsection",
 CellChangeTimes->{{3.7840084589598804`*^9, 3.7840084614861603`*^9}},
 FontSize->36,ExpressionUUID->"ef77df4f-678b-4509-ab16-6e2d12f47b12"],

Cell[TextData[StyleBox["Laplacesche Entwicklungssatz",
 FontWeight->"Bold"]], "Item",
 CellChangeTimes->{{3.7840084823134203`*^9, 3.7840085163174562`*^9}, {
  3.7840107638784657`*^9, 3.7840107773454475`*^9}},
 FontSize->24,ExpressionUUID->"5f8ef99a-8be2-4550-a41e-dacbd6d68eec"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"A", "\[Element]", 
    RowBox[{"M", "(", 
     RowBox[{
      RowBox[{"n", "\[Cross]", "n"}], ",", "\[DoubleStruckCapitalC]"}], 
     ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "111521cc-ea1a-4a3f-bd49-3f75192ff5fa"],
 "; ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "\[GreaterEqual]", "2"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "f5c7aacf-c8d1-4eb5-a7e6-14323cc1a671"],
 "; ",
 Cell[BoxData[
  FormBox[
   RowBox[{"A", "=", 
    RowBox[{"(", 
     SubscriptBox["a", 
      RowBox[{"i", " ", "j"}]], ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "652d690f-ff09-4944-9a2d-024810f78731"]
}], "Text",
 CellChangeTimes->{{3.7840108879136505`*^9, 3.7840109224722114`*^9}},
 FontSize->36,
 Background->RGBColor[
  0.88, 1, 0.88],ExpressionUUID->"b37a6732-e623-43c8-a930-a7a0a8ad4afe"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"det", " ", 
     RowBox[{"(", "A", ")"}]}], "=", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"i", "=", "1"}], "n"], 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"-", "1"}], ")"}], 
       RowBox[{"i", "+", "j"}]], 
      SubscriptBox["a", 
       RowBox[{"i", " ", "j"}]], 
      RowBox[{"det", "(", 
       SubsuperscriptBox["A", 
        RowBox[{"i", " ", "j"}], "/"], ")"}]}]}]}], ")"}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"6eeb4c36-a4af-46cc-8fe3-8ac60110e106"]], \
"Text",
 CellChangeTimes->{{3.7840084292892466`*^9, 3.784008438465701*^9}, 
   3.784008480371642*^9, {3.7840085439359293`*^9, 3.78400856851116*^9}, {
   3.7840107891328773`*^9, 3.7840108759935365`*^9}, {3.7840109247959948`*^9, 
   3.7840109258222494`*^9}},
 FontSize->36,
 Background->RGBColor[
  0.88, 1, 0.88],ExpressionUUID->"c6d1ad7c-5d37-4012-9366-b7326ff209e5"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"det", " ", 
     RowBox[{"(", "A", ")"}]}], "=", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"j", "=", "1"}], "n"], 
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"-", "1"}], ")"}], 
       RowBox[{"i", "+", "j"}]], 
      SubscriptBox["a", 
       RowBox[{"i", " ", "j"}]], 
      RowBox[{"det", "(", 
       SubsuperscriptBox["A", 
        RowBox[{"i", " ", "j"}], "/"], ")"}]}]}]}], ")"}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"64a94efc-2734-47cb-b3db-f1812877c480"]], \
"Text",
 CellChangeTimes->{{3.7840084292892466`*^9, 3.784008438465701*^9}, 
   3.784008480371642*^9, {3.7840085439359293`*^9, 3.78400856851116*^9}, {
   3.7840107891328773`*^9, 3.7840108759935365`*^9}, {3.7840109247959948`*^9, 
   3.7840109258222494`*^9}, {3.784011099513682*^9, 3.784011099513682*^9}},
 FontSize->36,
 Background->RGBColor[
  0.88, 1, 0.88],ExpressionUUID->"893982eb-e681-4c04-863f-aa6d3b4ac209"]
}, Closed]],

Cell[CellGroupData[{

Cell["Solution", "Chapter",
 CellChangeTimes->{{3.78107812801484*^9, 3.78107812969002*^9}, {
  3.7818707836368647`*^9, 
  3.7818707849489512`*^9}},ExpressionUUID->"d8b49904-adaf-4b5e-8940-\
a6a12a403501"],

Cell["\<\
Using Laplacesche Entwicklungssatz on the first row we obtain:\
\>", "Text",
 CellChangeTimes->{{3.784011081431052*^9, 3.78401112147793*^9}},
 FontSize->36,ExpressionUUID->"e96ae331-ea33-4c78-b4e2-03605925a677"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"det", " ", "A"}], "=", 
   RowBox[{
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], 
      RowBox[{"1", "+", "1"}]], "\[Lambda]", " ", 
     RowBox[{"det", "(", 
      RowBox[{"(", GridBox[{
         {"\[Lambda]", "\[CenterEllipsis]", "0", "0", 
          SubscriptBox["a", "1"]},
         {
          RowBox[{"-", "1"}], "\[CenterEllipsis]", "0", "0", 
          SubscriptBox["a", "2"]},
         {"\[VerticalEllipsis]", "\[DescendingEllipsis]", 
          "\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[VerticalEllipsis]"},
         {"0", "\[CenterEllipsis]", 
          RowBox[{"-", "1"}], "\[Lambda]", 
          SubscriptBox["a", 
           RowBox[{"n", "-", "2"}]]},
         {"0", "\[CenterEllipsis]", "0", 
          RowBox[{"-", "1"}], 
          RowBox[{"\[Lambda]", "+", 
           SubscriptBox["a", 
            RowBox[{"n", "-", "1"}]]}]}
        }], ")"}], ")"}]}], "+", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], 
      RowBox[{"n", "+", "1"}]], 
     SubscriptBox["a", "0"], " ", 
     RowBox[{"det", "(", 
      RowBox[{"(", GridBox[{
         {
          RowBox[{"-", "1"}], "\[Lambda]", "\[CenterEllipsis]", "0", "0"},
         {"0", 
          RowBox[{"-", "1"}], "\[CenterEllipsis]", "0", "0"},
         {"\[VerticalEllipsis]", "\[VerticalEllipsis]", 
          "\[DescendingEllipsis]", "\[VerticalEllipsis]", 
          "\[VerticalEllipsis]"},
         {"0", "0", "\[CenterEllipsis]", 
          RowBox[{"-", "1"}], "\[Lambda]"},
         {"0", "0", "\[CenterEllipsis]", "0", 
          RowBox[{"-", "1"}]}
        }], ")"}], ")"}]}]}]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"75b95b8f-c898-4246-b1de-0a0467918040"]], \
"Text",
 CellChangeTimes->{
  3.781870813002388*^9, {3.7818708992379203`*^9, 3.7818709745025997`*^9}, {
   3.7825530048415704`*^9, 3.7825530130805426`*^9}, {3.7829936556815205`*^9, 
   3.7829936971762877`*^9}, {3.782993816926934*^9, 3.7829938728825407`*^9}, 
   3.7834226312694626`*^9, {3.7834227227163525`*^9, 3.783422881411496*^9}, {
   3.7840109740472555`*^9, 3.7840110706299076`*^9}, {3.784011212223197*^9, 
   3.7840112122241955`*^9}},
 FontSize->36,ExpressionUUID->"b6cf728d-97f9-4d86-b638-75b7aae1cd74"],

Cell[TextData[{
 "The first matrix is of the same form as ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "b544b38c-8324-4b83-a35a-5081391c11c0"],
 ", only the last column starts with ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "1"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "3f58bdeb-1097-46c5-8f4d-ebe9bc3a582d"],
 ", not ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "0"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "0576ab7f-0edc-4b28-adff-ca84a9a72cd7"],
 ". The second matrix, let us call it ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[CapitalLambda]", 
    RowBox[{"n", "-", "1"}]], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "d9ac34ad-3217-4941-aac4-9c2f9950fd01"],
 ", contains no ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["a", "k"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "5369fdec-167b-4ca3-ab83-6914981184af"],
 " and it is easy to calculate its determinant. Specifically:"
}], "Text",
 CellChangeTimes->{{3.7840111354236293`*^9, 3.784011201199724*^9}, {
  3.7840114971849594`*^9, 3.784011507276005*^9}, {3.784012244334545*^9, 
  3.7840122445180545`*^9}},
 FontSize->36,ExpressionUUID->"0d5e5bd8-cddd-419f-92f9-47e54729ae02"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"det", "(", 
    RowBox[{"(", GridBox[{
       {
        RowBox[{"-", "1"}], "\[Lambda]", "\[CenterEllipsis]", "0", "0"},
       {"0", 
        RowBox[{"-", "1"}], "\[CenterEllipsis]", "0", "0"},
       {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[DescendingEllipsis]",
         "\[VerticalEllipsis]", "\[VerticalEllipsis]"},
       {"0", "0", "\[CenterEllipsis]", 
        RowBox[{"-", "1"}], "\[Lambda]"},
       {"0", "0", "\[CenterEllipsis]", "0", 
        RowBox[{"-", "1"}]}
      }], ")"}], ")"}], "=", 
   RowBox[{
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], 
      RowBox[{"1", "+", "1"}]], 
     RowBox[{"(", 
      RowBox[{"-", "1"}], ")"}], 
     RowBox[{"det", "(", 
      RowBox[{"(", GridBox[{
         {
          RowBox[{"-", "1"}], "\[CenterEllipsis]", "0", "0"},
         {"\[VerticalEllipsis]", "\[DescendingEllipsis]", 
          "\[VerticalEllipsis]", "\[VerticalEllipsis]"},
         {"0", "\[CenterEllipsis]", 
          RowBox[{"-", "1"}], "\[Lambda]"},
         {"0", "\[CenterEllipsis]", "0", 
          RowBox[{"-", "1"}]}
        }], ")"}], ")"}]}], "+", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], 
      RowBox[{"1", "+", "2"}]], "\[Lambda]", " ", 
     RowBox[{"det", "(", 
      RowBox[{"(", GridBox[{
         {"0", "\[CenterEllipsis]", "0", "0"},
         {"\[VerticalEllipsis]", "\[DescendingEllipsis]", 
          "\[VerticalEllipsis]", "\[VerticalEllipsis]"},
         {"0", "\[CenterEllipsis]", 
          RowBox[{"-", "1"}], "\[Lambda]"},
         {"0", "\[CenterEllipsis]", "0", 
          RowBox[{"-", "1"}]}
        }], ")"}], ")"}]}]}]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"eda9712c-b15a-4bf8-8341-f47ae0bdd2fa"]], \
"Text",
 CellChangeTimes->{{3.7840112027206154`*^9, 3.784011262372056*^9}},
 FontSize->36,ExpressionUUID->"b61ecf0c-dde5-416f-88c7-75b8a558db1f"],

Cell[TextData[{
 "In here, the first column of the second matrix is full of zeros and the \
resulting determinant is thus also zero. The first matrix is of the same form \
as ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[CapitalLambda]", 
    RowBox[{"n", "-", "1"}]], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "3ca4795a-2956-45e7-820c-2f56bacb0c22"],
 ", only its dimension is not ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "-", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "7b53036b-29ab-445a-aaf2-ca4d09c63541"],
 ", but ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "-", "2"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "dad0638e-05e0-4f43-a99b-3ca5896933e7"],
 ". So we get relation:"
}], "Text",
 CellChangeTimes->{{3.784011278415142*^9, 3.784011338780671*^9}, {
  3.7840115279696145`*^9, 3.7840115513171635`*^9}},
 FontSize->36,ExpressionUUID->"e97206ad-5004-4970-a602-ec756ebdc16e"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"det", "(", 
    SubscriptBox["\[CapitalLambda]", 
     RowBox[{"n", "-", "1"}]], ")"}], "=", 
   RowBox[{
    RowBox[{"det", "(", 
     RowBox[{"(", GridBox[{
        {
         RowBox[{"-", "1"}], "\[Lambda]", "\[CenterEllipsis]", "0", "0"},
        {"0", 
         RowBox[{"-", "1"}], "\[CenterEllipsis]", "0", "0"},
        {"\[VerticalEllipsis]", "\[VerticalEllipsis]", 
         "\[DescendingEllipsis]", "\[VerticalEllipsis]", 
         "\[VerticalEllipsis]"},
        {"0", "0", "\[CenterEllipsis]", 
         RowBox[{"-", "1"}], "\[Lambda]"},
        {"0", "0", "\[CenterEllipsis]", "0", 
         RowBox[{"-", "1"}]}
       }], ")"}], ")"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], 
      RowBox[{"det", "(", 
       RowBox[{"(", GridBox[{
          {
           RowBox[{"-", "1"}], "\[CenterEllipsis]", "0", "0"},
          {"\[VerticalEllipsis]", "\[DescendingEllipsis]", 
           "\[VerticalEllipsis]", "\[VerticalEllipsis]"},
          {"0", "\[CenterEllipsis]", 
           RowBox[{"-", "1"}], "\[Lambda]"},
          {"0", "\[CenterEllipsis]", "0", 
           RowBox[{"-", "1"}]}
         }], ")"}], ")"}]}], "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], " ", 
      RowBox[{"det", "(", 
       SubscriptBox["\[CapitalLambda]", 
        RowBox[{"n", "-", "2"}]], ")"}]}]}]}]}], 
  TraditionalForm]],ExpressionUUID->"08bf1630-b9ed-4538-9b79-77ed9898cfa6"]], \
"Text",
 CellChangeTimes->{{3.7840113402806997`*^9, 3.7840113630038786`*^9}, {
  3.7840115534464664`*^9, 3.784011563911474*^9}},
 FontSize->36,ExpressionUUID->"b457bb98-ae02-480b-9639-6bfb002773e4"],

Cell[TextData[{
 "It is not hard to see, that if we continue with the calculation of the \
determinant on the right-hand side, we obtain for ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[CapitalLambda]", 
    RowBox[{"n", "-", "1"}]], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "e438b2a0-b402-478a-a5ee-cb7007438bc0"],
 ":"
}], "Text",
 CellChangeTimes->{{3.7840113713974257`*^9, 3.784011408325681*^9}, {
  3.7840115721634007`*^9, 3.7840115729213734`*^9}, {3.7840122675873485`*^9, 
  3.78401226785866*^9}},
 FontSize->36,ExpressionUUID->"2a53e0f9-1553-4625-be41-b7317d23d327"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"det", "(", 
    SubscriptBox["\[CapitalLambda]", 
     RowBox[{"n", "-", "1"}]], ")"}], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"-", "1"}], ")"}], 
     RowBox[{"det", "(", 
      SubscriptBox["\[CapitalLambda]", 
       RowBox[{"n", "-", "2"}]], ")"}]}], "=", 
    RowBox[{
     RowBox[{
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"-", "1"}], ")"}], "2"], 
      RowBox[{"det", "(", 
       SubscriptBox["\[CapitalLambda]", 
        RowBox[{"n", "-", "3"}]], ")"}]}], "=", 
     RowBox[{"\[CenterEllipsis]", "=", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"-", "1"}], ")"}], 
       RowBox[{"n", "-", "1"}]]}]}]}]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"0cb33ef8-b8c3-4fea-a2df-ce080c325862"]], \
"Text",
 CellChangeTimes->{{3.784011414244814*^9, 3.7840114499313574`*^9}, {
  3.784011577714553*^9, 3.78401160463654*^9}},
 FontSize->36,ExpressionUUID->"0bfc5100-432a-4ee0-9d99-9e85ead3422d"],

Cell[TextData[{
 "We use this result to calculate the determinant of the original matrix ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "797dfb5e-c74f-4de7-92f8-14a28b60d9d8"],
 ":"
}], "Text",
 CellChangeTimes->{{3.784011609199335*^9, 3.7840116235898438`*^9}},
 FontSize->36,ExpressionUUID->"5c6940d2-1482-4d20-8f6f-f5cde2f54570"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"det", " ", "A"}], "=", 
   RowBox[{
    RowBox[{"\[Lambda]", " ", 
     RowBox[{"det", "(", 
      RowBox[{"(", GridBox[{
         {"\[Lambda]", "\[CenterEllipsis]", "0", "0", 
          SubscriptBox["a", "1"]},
         {
          RowBox[{"-", "1"}], "\[CenterEllipsis]", "0", "0", 
          SubscriptBox["a", "2"]},
         {"\[VerticalEllipsis]", "\[DescendingEllipsis]", 
          "\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[VerticalEllipsis]"},
         {"0", "\[CenterEllipsis]", 
          RowBox[{"-", "1"}], "\[Lambda]", 
          SubscriptBox["a", 
           RowBox[{"n", "-", "2"}]]},
         {"0", "\[CenterEllipsis]", "0", 
          RowBox[{"-", "1"}], 
          RowBox[{"\[Lambda]", "+", 
           SubscriptBox["a", 
            RowBox[{"n", "-", "1"}]]}]}
        }], ")"}], ")"}]}], "+", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], 
      RowBox[{"n", "+", "1"}]], 
     SubscriptBox["a", "0"], " ", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], 
      RowBox[{"n", "-", "1"}]]}]}]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"9970db7f-d6ca-4d37-8ae7-01b09c5abe8c"]], \
"Text",
 CellChangeTimes->{{3.7840116329278655`*^9, 3.784011642998961*^9}},
 FontSize->36,ExpressionUUID->"ff643599-e92e-409e-9eda-917a29f6232e"],

Cell[TextData[{
 "Let us define ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["A", "0"], ":=", "A"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "d2c29613-6ecc-4d67-a29e-99214f77818b"],
 " and the matrix on the right-hand side as ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["A", "1"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "dfb51fc2-7f02-4d1f-a655-2a2133f8efbf"],
 ". Then:"
}], "Text",
