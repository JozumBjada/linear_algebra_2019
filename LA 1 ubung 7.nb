     RowBox[{
      RowBox[{"f", "\[Element]", 
       RowBox[{
        SuperscriptBox["\[ScriptCapitalC]", "\[Infinity]"], "(", 
        "\[DoubleStruckCapitalR]", ")"}]}], "|", 
      RowBox[{"\[ForAll]", 
       RowBox[{"x", "\[Element]", 
        RowBox[{"\[DoubleStruckCapitalR]", ":", 
         RowBox[{
          SubsuperscriptBox["\[Integral]", "0", "x"], 
          RowBox[{
           RowBox[{"f", "(", "y", ")"}], 
           RowBox[{"\[DifferentialD]", "y"}]}]}]}]}]}]}], "=", "0"}], "}"}]}],
   TraditionalForm]],
