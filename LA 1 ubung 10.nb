Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   StyleBox[
    RowBox[{
     SubscriptBox["P", "A"], "(", "t", ")"}],
    FontWeight->"Bold"], "=", 
   RowBox[{
    RowBox[{"det", "(", 
     RowBox[{"A", "\[Minus]", 
      RowBox[{"(", 
       RowBox[{
       "t", " ", "\[CenterDot]", " ", 
        "\[DoubleStruckCapitalI]\[DoubleStruckD]"}], ")"}]}], ")"}], "=", 
    RowBox[{
     RowBox[{"det", " ", 
      RowBox[{"(", GridBox[{
         {
          RowBox[{
           RowBox[{"-", "2"}], "-", "t"}], "1", "3"},
         {
          RowBox[{"-", "3"}], 
          RowBox[{"2", "-", "t"}], "3"},
         {
          RowBox[{"-", "1"}], "1", 
          RowBox[{"2", "-", "t"}]}
        }], ")"}]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"-", "2"}], "-", "t"}], ")"}], "\[CenterDot]", "det"}], 
        " ", 
        RowBox[{"(", GridBox[{
           {
            RowBox[{"2", "-", "t"}], "3"},
           {"1", 
            RowBox[{"2", "-", "t"}]}
          }], ")"}]}], "-", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"-", "3"}], ")"}], "\[CenterDot]", "det"}], " ", 
        RowBox[{"(", GridBox[{
           {"1", "3"},
           {"1", 
            RowBox[{"2", "-", "t"}]}
          }], ")"}]}], "+", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"-", "1"}], ")"}], "\[CenterDot]", "det"}], " ", 
        RowBox[{"(", GridBox[{
           {"1", "3"},
           {
            RowBox[{"2", "-", "t"}], "3"}
          }], ")"}]}]}], "=", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"-", "2"}], "-", "t"}], ")"}], "\[CenterDot]", 
         RowBox[{"(", 
          RowBox[{
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"2", "-", "t"}], ")"}], "2"], "-", "3"}], ")"}]}], "-", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"-", "3"}], ")"}], "\[CenterDot]", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"2", "-", "t"}], ")"}], "-", "3"}], ")"}]}], "+", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"-", "1"}], ")"}], "\[CenterDot]", 
         RowBox[{"(", 
          RowBox[{"3", "-", 
           RowBox[{"3", 
            RowBox[{"(", 
             RowBox[{"2", "-", "t"}], ")"}]}]}], ")"}]}]}], "=", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{"2", "-", "t"}], ")"}], "\[CenterDot]", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             RowBox[{"-", 
              RowBox[{"(", 
               RowBox[{"2", "+", "t"}], ")"}]}], 
             RowBox[{"(", 
              RowBox[{"2", "-", "t"}], ")"}]}], "+", "3", "+", "3"}], ")"}]}],
          "+", 
         RowBox[{"3", 
          RowBox[{"(", 
           RowBox[{"2", "+", "t"}], ")"}]}], "-", "9", "-", "3"}], "=", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"2", "-", "t"}], ")"}], "\[CenterDot]", 
           RowBox[{"(", 
            RowBox[{
             SuperscriptBox["t", "2"], "+", "2"}], ")"}]}], "+", 
          RowBox[{"3", " ", "t"}], "-", "6"}], "=", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"2", "-", "t"}], ")"}], "\[CenterDot]", 
           RowBox[{"(", 
            RowBox[{
             SuperscriptBox["t", "2"], "-", "1"}], ")"}]}], "=", 
          StyleBox[
           RowBox[{
            RowBox[{"-", 
             RowBox[{"(", 
              RowBox[{"t", "-", "2"}], ")"}]}], 
            RowBox[{"(", 
             RowBox[{"t", "-", "1"}], ")"}], 
            RowBox[{"(", 
             RowBox[{"t", "+", "1"}], ")"}]}],
           FontWeight->"Bold"]}]}]}]}]}]}]}]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"d9816823-e7b9-43a5-b52e-aeaba98a0a92"]], \
"Text",
