082ecf6a2e43"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   StyleBox[
    RowBox[{"det", " ", 
     RowBox[{"A", "(", 
      RowBox[{
       SubscriptBox["\[Lambda]", "1"], ",", 
       SubscriptBox["\[Lambda]", "2"]}], ")"}]}],
    FontWeight->"Bold"], "=", 
   RowBox[{
    RowBox[{"det", " ", 
     RowBox[{"(", GridBox[{
        {"1", 
         SubscriptBox["\[Lambda]", "1"]},
        {"1", 
         SubscriptBox["\[Lambda]", "2"]}
       }], ")"}]}], "=", 
    StyleBox[
     RowBox[{
      SubscriptBox["\[Lambda]", "2"], "-", 
      SubscriptBox["\[Lambda]", "1"]}],
     FontWeight->"Bold"]}]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"921a49a8-57d4-4eae-a32d-c7aa2f0e2803"]], \
"Text",
 CellChangeTimes->{{3.784722957083559*^9, 3.7847231467815075`*^9}},
 FontSize->36,ExpressionUUID->"b29d493f-98f9-4254-9395-288e54e8b6d4"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"det", " ", 
    RowBox[{"A", "(", 
     RowBox[{
      SubscriptBox["\[Lambda]", "1"], ",", 
      SubscriptBox["\[Lambda]", "2"], ",", 
      SubscriptBox["\[Lambda]", "3"]}], ")"}]}], "=", 
   RowBox[{
    RowBox[{"det", "(", GridBox[{
       {"1", 
        SubscriptBox["\[Lambda]", "1"], 
        SubsuperscriptBox["\[Lambda]", "1", "2"]},
       {"1", 
        SubscriptBox["\[Lambda]", "2"], 
        SubsuperscriptBox["\[Lambda]", "2", "2"]},
       {"1", 
        SubscriptBox["\[Lambda]", "3"], 
        SubsuperscriptBox["\[Lambda]", "3", "2"]}
      }], ")"}], "=", 
    RowBox[{
     RowBox[{"det", "(", GridBox[{
        {"1", 
         SubscriptBox["\[Lambda]", "1"], 
         SubsuperscriptBox["\[Lambda]", "1", "2"]},
        {"0", 
         RowBox[{
          SubscriptBox["\[Lambda]", "2"], "-", 
          SubscriptBox["\[Lambda]", "1"]}], 
         RowBox[{
          SubsuperscriptBox["\[Lambda]", "2", "2"], "-", 
          SubsuperscriptBox["\[Lambda]", "1", "2"]}]},
        {"0", 
         RowBox[{
          SubscriptBox["\[Lambda]", "3"], "-", 
          SubscriptBox["\[Lambda]", "1"]}], 
         RowBox[{
          SubsuperscriptBox["\[Lambda]", "3", "2"], "-", 
          SubsuperscriptBox["\[Lambda]", "1", "2"]}]}
       }], ")"}], "=", 
     RowBox[{
      RowBox[{"det", "(", GridBox[{
         {
          RowBox[{
           SubscriptBox["\[Lambda]", "2"], "-", 
           SubscriptBox["\[Lambda]", "1"]}], 
          RowBox[{
           SubsuperscriptBox["\[Lambda]", "2", "2"], "-", 
           SubsuperscriptBox["\[Lambda]", "1", "2"]}]},
         {
          RowBox[{
           SubscriptBox["\[Lambda]", "3"], "-", 
           SubscriptBox["\[Lambda]", "1"]}], 
          RowBox[{
           SubsuperscriptBox["\[Lambda]", "3", "2"], "-", 
           SubsuperscriptBox["\[Lambda]", "1", "2"]}]}
        }], ")"}], "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["\[Lambda]", "2"], "-", 
         SubscriptBox["\[Lambda]", "1"]}], ")"}], 
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["\[Lambda]", "3"], "-", 
         SubscriptBox["\[Lambda]", "1"]}], ")"}], 
       RowBox[{"det", "(", GridBox[{
          {"1", 
           RowBox[{
            SubscriptBox["\[Lambda]", "1"], "+", 
            SubscriptBox["\[Lambda]", "2"]}]},
          {"1", 
           RowBox[{
            SubscriptBox["\[Lambda]", "1"], "+", 
            SubscriptBox["\[Lambda]", "3"]}]}
         }], ")"}]}]}]}]}]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"a8619787-c76f-474a-bceb-6b8ca3e0e072"]], \
"Text",
 CellChangeTimes->{{3.7847231555155153`*^9, 3.7847232866379023`*^9}},
 FontSize->36,ExpressionUUID->"231efa0f-bc1b-4d4b-8423-52bc4fff4b94"],

Cell["\<\
where in the last equality we used the formula mentioned in section \
\[OpenCurlyDoubleQuote]Useful formulas\[CloseCurlyDoubleQuote] for rows. \
Furthermore:\
\>", "Text",
 CellChangeTimes->{{3.784723304704866*^9, 3.784723349331325*^9}, {
  3.7847281105519476`*^9, 3.7847281125372877`*^9}},
 FontSize->36,ExpressionUUID->"203d2153-a584-41da-8bd4-d0fde3d3f110"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"det", "(", GridBox[{
       {"1", 
        RowBox[{
         SubscriptBox["\[Lambda]", "1"], "+", 
         SubscriptBox["\[Lambda]", "2"]}]},
       {"1", 
        RowBox[{
         SubscriptBox["\[Lambda]", "1"], "+", 
         SubscriptBox["\[Lambda]", "3"]}]}
      }], ")"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["\[Lambda]", "1"], "+", 
        SubscriptBox["\[Lambda]", "3"]}], ")"}], "-", 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["\[Lambda]", "1"], "+", 
        SubscriptBox["\[Lambda]", "2"]}], ")"}]}], "=", 
     RowBox[{
      SubscriptBox["\[Lambda]", "3"], "-", 
      SubscriptBox["\[Lambda]", "2"]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "ac55f56d-f05b-4a71-8f8a-f22dbf5de45f"],
 "  and we get:"
}], "Text",
 CellChangeTimes->{{3.7847233504610925`*^9, 3.7847233830527797`*^9}},
 FontSize->36,ExpressionUUID->"0105fb81-0c1d-44c4-ad48-14a8459f495d"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"det", " ", 
    RowBox[{"A", "(", 
     RowBox[{
      SubscriptBox["\[Lambda]", "1"], ",", 
      SubscriptBox["\[Lambda]", "2"], ",", 
      SubscriptBox["\[Lambda]", "3"]}], ")"}]}], "=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["\[Lambda]", "2"], "-", 
      SubscriptBox["\[Lambda]", "1"]}], ")"}], 
    RowBox[{"(", 
