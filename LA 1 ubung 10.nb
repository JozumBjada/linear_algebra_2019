Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"A", "-", 
    RowBox[{
     SubscriptBox["\[Lambda]", "1"], "\[CenterDot]", 
     "\[DoubleStruckCapitalI]\[DoubleStruckD]"}]}], "=", 
   RowBox[{
    RowBox[{"(", GridBox[{
       {
        RowBox[{
         RowBox[{"-", "2"}], "-", 
         SubscriptBox["\[Lambda]", "1"]}], "1", "3"},
       {
        RowBox[{"-", "3"}], 
        RowBox[{"2", "-", 
         SubscriptBox["\[Lambda]", "1"]}], "3"},
       {
        RowBox[{"-", "1"}], "1", 
        RowBox[{"2", "-", 
         SubscriptBox["\[Lambda]", "1"]}]}
      }], ")"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", GridBox[{
         {
          RowBox[{"-", "1"}], "1", "3"},
         {
          RowBox[{"-", "3"}], "3", "3"},
         {
          RowBox[{"-", "1"}], "1", "3"}
        }], ")"}], "~", 
      RowBox[{"(", GridBox[{
         {
          RowBox[{"-", "1"}], "1", "3"},
