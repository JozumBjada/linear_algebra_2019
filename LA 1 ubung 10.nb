Cell[TextData[{
 "It holds that ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{
      SubsuperscriptBox["\[Lambda]", "i", "k"], "-", 
      SubsuperscriptBox["\[Lambda]", "j", "k"]}], 
     RowBox[{
      SubscriptBox["\[Lambda]", "i"], "-", 
      SubscriptBox["\[Lambda]", "j"]}]], "=", 
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"m", "=", "0"}], 
      RowBox[{"k", "-", "1"}]], 
     RowBox[{
      SubsuperscriptBox["\[Lambda]", "i", "m"], 
      SubsuperscriptBox["\[Lambda]", "j", 
       RowBox[{"k", "-", "1", "-", "m"}]]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "4e7acb4c-93c3-4879-9d28-7e0fe351d482"],
 " for any ",
 Cell[BoxData[
  FormBox[
   RowBox[{"k", "\[GreaterEqual]", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "e8f645e4-24dd-4101-979c-0d98c033cbd8"],
 ". The matrix in the last term can be further simplified using the formula \
in section \[OpenCurlyDoubleQuote]Useful formulas\[CloseCurlyDoubleQuote] \
applied on columns. For instance, we can subtract the ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["\[Lambda]", "1"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "cf61ce57-11ee-41c1-b519-8696621d3c05"],
 "-multiple of the first column from the second column. Analogously for all \
other columns. The determinant is not affected by these transformations, so \
we get:"
}], "Text",
 CellChangeTimes->{{3.7847237775545177`*^9, 3.7847238199802837`*^9}, {
  3.7847238678760414`*^9, 3.784724022609527*^9}, {3.7847281822823505`*^9, 
  3.7847281843996058`*^9}},
 FontSize->36,ExpressionUUID->"5a0d61e7-bd8e-48cd-a2a1-5a3363446e0f"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"det", " ", 
    RowBox[{"(", GridBox[{
       {"1", 
        RowBox[{
         SubscriptBox["\[Lambda]", "2"], "+", 
         SubscriptBox["\[Lambda]", "1"]}], "\[CenterEllipsis]", 
        FractionBox[
         RowBox[{
          SubsuperscriptBox["\[Lambda]", "2", 
           RowBox[{"n", "-", "1"}]], "-", 
          SubsuperscriptBox["\[Lambda]", "1", 
           RowBox[{"n", "-", "1"}]]}], 
         RowBox[{
          SubscriptBox["\[Lambda]", "2"], "-", 
          SubscriptBox["\[Lambda]", "1"]}]]},
       {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[DescendingEllipsis]",
         "\[VerticalEllipsis]"},
       {"1", 
        RowBox[{
         SubscriptBox["\[Lambda]", "n"], "+", 
         SubscriptBox["\[Lambda]", "1"]}], "\[CenterEllipsis]", 
        FractionBox[
         RowBox[{
          SubsuperscriptBox["\[Lambda]", "n", 
           RowBox[{"n", "-", "1"}]], "-", 
          SubsuperscriptBox["\[Lambda]", "1", 
           RowBox[{"n", "-", "1"}]]}], 
         RowBox[{
          SubscriptBox["\[Lambda]", "n"], "-", 
          SubscriptBox["\[Lambda]", "1"]}]]}
      }], ")"}]}], "=", 
   RowBox[{"det", " ", 
    RowBox[{"(", GridBox[{
       {"1", 
        SubscriptBox["\[Lambda]", "2"], "\[CenterEllipsis]", 
        SubsuperscriptBox["\[Lambda]", "2", 
         RowBox[{"n", "-", "2"}]]},
       {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[DescendingEllipsis]",
         "\[VerticalEllipsis]"},
       {"1", 
        SubscriptBox["\[Lambda]", "n"], "\[CenterEllipsis]", 
        SubsuperscriptBox["\[Lambda]", "n", 
         RowBox[{"n", "-", "2"}]]}
      }], ")"}]}]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"c7eb2e75-ad85-440e-bd49-4f31fce49a48"]], \
"Text",
