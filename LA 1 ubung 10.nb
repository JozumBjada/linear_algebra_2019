Cell[TextData[StyleBox["Properties of determinant",
 FontWeight->"Bold"]], "Item",
 CellChangeTimes->{{3.7840084823134203`*^9, 3.7840085163174562`*^9}, {
  3.7840107638784657`*^9, 3.7840107773454475`*^9}, {3.784722691714939*^9, 
  3.784722699081831*^9}},
 FontSize->24,ExpressionUUID->"3f869ace-bc00-4e87-abfb-8c2aee016726"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"det", "(", 
    RowBox[{"(", GridBox[{
       {
        SubscriptBox["a", 
         RowBox[{"1", ",", "1"}]], 
        RowBox[{"\[Mu]", " ", 
         SubscriptBox["a", 
          RowBox[{"1", ",", "2"}]]}], "\[CenterEllipsis]", 
        SubscriptBox["a", 
         RowBox[{"1", ",", "n"}]]},
       {
        SubscriptBox["a", 
         RowBox[{"2", ",", "1"}]], 
        RowBox[{"\[Mu]", " ", 
         SubscriptBox["a", 
          RowBox[{"2", ",", "2"}]]}], "\[CenterEllipsis]", 
        SubscriptBox["a", 
         RowBox[{"2", ",", "n"}]]},
       {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[DescendingEllipsis]",
         "\[VerticalEllipsis]"},
       {
        SubscriptBox["a", 
         RowBox[{"n", ",", "1"}]], 
        RowBox[{"\[Mu]", " ", 
         SubscriptBox["a", 
          RowBox[{"n", ",", "2"}]]}], "\[CenterEllipsis]", 
        SubscriptBox["a", 
         RowBox[{"n", ",", "n"}]]}
      }], ")"}], ")"}], "=", 
   RowBox[{"\[Mu]", " ", "\[CenterDot]", " ", 
    RowBox[{"det", "(", 
     RowBox[{"(", GridBox[{
        {
         SubscriptBox["a", 
          RowBox[{"1", ",", "1"}]], 
         SubscriptBox["a", 
          RowBox[{"1", ",", "2"}]], "\[CenterEllipsis]", 
         SubscriptBox["a", 
          RowBox[{"1", ",", "n"}]]},
        {
         SubscriptBox["a", 
          RowBox[{"2", ",", "1"}]], 
         SubscriptBox["a", 
          RowBox[{"2", ",", "2"}]], "\[CenterEllipsis]", 
         SubscriptBox["a", 
          RowBox[{"2", ",", "n"}]]},
        {"\[VerticalEllipsis]", "\[VerticalEllipsis]", 
         "\[DescendingEllipsis]", "\[VerticalEllipsis]"},
        {
         SubscriptBox["a", 
          RowBox[{"n", ",", "1"}]], 
         SubscriptBox["a", 
          RowBox[{"n", ",", "2"}]], "\[CenterEllipsis]", 
         SubscriptBox["a", 
          RowBox[{"n", ",", "n"}]]}
       }], ")"}], ")"}]}]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"5c6e7f14-cb97-45e8-a694-5ec93318ed7f"]], \
"Text",
