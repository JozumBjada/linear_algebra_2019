Cell[CellGroupData[{

Cell["a)", "Section",
 CellChangeTimes->{{3.7847226027223015`*^9, 
  3.7847226053117166`*^9}},ExpressionUUID->"f3bd4dd4-b0e7-4207-9ad9-\
925c68833f84"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"det", " ", 
    RowBox[{"A", "(", 
     RowBox[{
      RowBox[{"\[Mu]", " ", 
       SubscriptBox["\[Lambda]", "1"]}], ",", "\[Ellipsis]", ",", 
      RowBox[{"\[Mu]", " ", 
       SubscriptBox["\[Lambda]", "n"]}]}], ")"}]}], "=", 
   RowBox[{
    RowBox[{"det", " ", 
     RowBox[{"(", GridBox[{
        {"1", 
         RowBox[{"\[Mu]", " ", 
          SubscriptBox["\[Lambda]", "1"]}], 
         RowBox[{
          SuperscriptBox["\[Mu]", "2"], 
          SubsuperscriptBox["\[Lambda]", "1", "2"]}], "\[CenterEllipsis]", 
         RowBox[{
          SuperscriptBox["\[Mu]", 
           RowBox[{"n", "-", "1"}]], 
          SubsuperscriptBox["\[Lambda]", "1", 
           RowBox[{"n", "-", "1"}]]}]},
        {"1", 
         RowBox[{"\[Mu]", " ", 
          SubscriptBox["\[Lambda]", "2"]}], 
         RowBox[{
          SuperscriptBox["\[Mu]", "2"], 
          SubsuperscriptBox["\[Lambda]", "2", "2"]}], "\[CenterEllipsis]", 
         RowBox[{
          SuperscriptBox["\[Mu]", 
           RowBox[{"n", "-", "1"}]], 
          SubsuperscriptBox["\[Lambda]", "2", 
           RowBox[{"n", "-", "1"}]]}]},
        {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[VerticalEllipsis]", 
         "\[DescendingEllipsis]", "\[VerticalEllipsis]"},
        {"1", 
         RowBox[{"\[Mu]", " ", 
          SubscriptBox["\[Lambda]", "n"]}], 
         RowBox[{
          SuperscriptBox["\[Mu]", "2"], " ", 
          SubsuperscriptBox["\[Lambda]", "n", "2"]}], "\[CenterEllipsis]", 
         RowBox[{
          SuperscriptBox["\[Mu]", 
           RowBox[{"n", "-", "1"}]], 
          SubsuperscriptBox["\[Lambda]", "n", 
           RowBox[{"n", "-", "1"}]]}]}
       }], ")"}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"\[Mu]", "\[CenterDot]", "det"}], " ", 
      RowBox[{"(", GridBox[{
         {"1", 
          SubscriptBox["\[Lambda]", "1"], 
          RowBox[{
           SuperscriptBox["\[Mu]", "2"], 
           SubsuperscriptBox["\[Lambda]", "1", "2"]}], "\[CenterEllipsis]", 
          RowBox[{
           SuperscriptBox["\[Mu]", 
            RowBox[{"n", "-", "1"}]], 
           SubsuperscriptBox["\[Lambda]", "1", 
            RowBox[{"n", "-", "1"}]]}]},
         {"1", 
          SubscriptBox["\[Lambda]", "2"], 
          RowBox[{
           SuperscriptBox["\[Mu]", "2"], 
           SubsuperscriptBox["\[Lambda]", "2", "2"]}], "\[CenterEllipsis]", 
          RowBox[{
           SuperscriptBox["\[Mu]", 
            RowBox[{"n", "-", "1"}]], 
           SubsuperscriptBox["\[Lambda]", "2", 
            RowBox[{"n", "-", "1"}]]}]},
         {"\[VerticalEllipsis]", "\[VerticalEllipsis]", "\[VerticalEllipsis]",
           "\[DescendingEllipsis]", "\[VerticalEllipsis]"},
         {"1", 
          SubscriptBox["\[Lambda]", "n"], 
          RowBox[{
           SuperscriptBox["\[Mu]", "2"], " ", 
           SubsuperscriptBox["\[Lambda]", "n", "2"]}], "\[CenterEllipsis]", 
          RowBox[{
           SuperscriptBox["\[Mu]", 
            RowBox[{"n", "-", "1"}]], 
           SubsuperscriptBox["\[Lambda]", "n", 
            RowBox[{"n", "-", "1"}]]}]}
        }], ")"}]}], "=", 
     RowBox[{"\[Ellipsis]", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"\[Mu]", "\[CenterDot]", 
         SuperscriptBox["\[Mu]", "2"], "\[CenterDot]", "\[Ellipsis]", 
         "\[CenterDot]", 
         SuperscriptBox["\[Mu]", 
          RowBox[{"n", "-", "1"}]], "\[CenterDot]", "det"}], " ", 
        RowBox[{"(", GridBox[{
           {"1", 
            SubscriptBox["\[Lambda]", "1"], 
            SubsuperscriptBox["\[Lambda]", "1", "2"], "\[CenterEllipsis]", 
            SubsuperscriptBox["\[Lambda]", "1", 
             RowBox[{"n", "-", "1"}]]},
           {"1", 
            SubscriptBox["\[Lambda]", "2"], 
            SubsuperscriptBox["\[Lambda]", "2", "2"], "\[CenterEllipsis]", 
            SubsuperscriptBox["\[Lambda]", "2", 
             RowBox[{"n", "-", "1"}]]},
           {"\[VerticalEllipsis]", "\[VerticalEllipsis]", 
            "\[VerticalEllipsis]", "\[DescendingEllipsis]", 
            "\[VerticalEllipsis]"},
           {"1", 
            SubscriptBox["\[Lambda]", "n"], 
            SubsuperscriptBox["\[Lambda]", "n", "2"], "\[CenterEllipsis]", 
            SubsuperscriptBox["\[Lambda]", "n", 
             RowBox[{"n", "-", "1"}]]}
          }], ")"}]}], "=", 
       RowBox[{
        RowBox[{
         SuperscriptBox["\[Mu]", 
          RowBox[{"1", "+", "2", "+", "\[Ellipsis]", "+", 
           RowBox[{"(", 
            RowBox[{"n", "-", "1"}], ")"}]}]], "\[CenterDot]", "det"}], " ", 
        RowBox[{"A", "(", 
         RowBox[{
          SubscriptBox["\[Lambda]", "1"], ",", "\[Ellipsis]", ",", 
          SubscriptBox["\[Lambda]", "n"]}], ")"}]}]}]}]}]}]}], 
  TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"9124992d-1b57-48b0-ad8f-d67bc4175274"]], \
"Text",
 CellChangeTimes->{{3.784011081431052*^9, 3.78401112147793*^9}, {
  3.784722635443246*^9, 3.7847226768684845`*^9}, {3.7847228536436596`*^9, 
  3.784722954174288*^9}, {3.78472804983924*^9, 3.7847280542627234`*^9}},
 FontSize->36,ExpressionUUID->"8dc408b0-6789-4062-91df-fede815a3d1a"],

Cell[TextData[{
 "To complete the proof, let\[CloseCurlyQuote]s sum the exponent up to get: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"1", "+", "2", "+", "\[Ellipsis]", "+", 
     RowBox[{"(", 
      RowBox[{"n", "-", "1"}], ")"}]}], "=", 
    RowBox[{
     FractionBox["n", "2"], 
     RowBox[{"(", 
      RowBox[{"n", "-", "1"}], ")"}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "bfd45a35-d0cc-4d36-8a23-b57c17d8ed7a"],
 "."
}], "Text",
 CellChangeTimes->{{3.784722957083559*^9, 3.7847229954143744`*^9}},
 FontSize->36,ExpressionUUID->"38427e8f-a683-434e-a739-09387a743251"]
}, Closed]],

