 FontSize->36,ExpressionUUID->"2adcda91-405c-421f-934b-2cd7dbcd64e9"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["P", "1"], "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["v", "1"], 
     SubsuperscriptBox["v", "1", "T"]}], "=", 
    RowBox[{
     RowBox[{
      FractionBox["1", "2"], 
      RowBox[{
       RowBox[{"(", GridBox[{
          {
           SqrtBox["3"]},
          {"1"}
         }], ")"}], "\[CenterDot]", 
       FractionBox["1", "2"]}], 
      RowBox[{"(", GridBox[{
         {
          SqrtBox["3"], "1"}
        }], ")"}]}], "=", 
     RowBox[{
      FractionBox["1", "4"], 
      RowBox[{"(", GridBox[{
         {"3", 
