0e13720aacf7"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["P", "A"], "(", "\[Lambda]", ")"}], "=", 
   RowBox[{
    RowBox[{"det", "(", 
     RowBox[{"A", "-", 
      RowBox[{"\[Lambda]", " ", "\[DoubleStruckCapitalI]\[DoubleStruckD]"}]}],
      ")"}], "=", 
    RowBox[{
     RowBox[{"det", "(", GridBox[{
        {
         RowBox[{"2", "-", "\[Lambda]"}], 
         RowBox[{"-", "\[ImaginaryI]"}], 
         RowBox[{"-", "\[ImaginaryI]"}]},
        {"\[ImaginaryI]", 
         RowBox[{"2", "-", "\[Lambda]"}], 
         RowBox[{"-", "1"}]},
        {"\[ImaginaryI]", 
         RowBox[{"-", "1"}], 
         RowBox[{"2", "-", "\[Lambda]"}]}
       }], ")"}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"2", "-", "\[Lambda]"}], ")"}], 
        RowBox[{"det", "(", GridBox[{
           {
            RowBox[{"2", "-", "\[Lambda]"}], 
            RowBox[{"-", "1"}]},
           {
            RowBox[{"-", "1"}], 
            RowBox[{"2", "-", "\[Lambda]"}]}
          }], ")"}]}], "-", 
       RowBox[{"\[ImaginaryI]", " ", 
        RowBox[{"det", "(", GridBox[{
           {
            RowBox[{"-", "\[ImaginaryI]"}], 
            RowBox[{"-", "\[ImaginaryI]"}]},
           {
            RowBox[{"-", "1"}], 
            RowBox[{"2", "-", "\[Lambda]"}]}
          }], ")"}]}], "+", 
       RowBox[{"\[ImaginaryI]", " ", 
        RowBox[{"det", "(", GridBox[{
           {
            RowBox[{"2", "-", "\[Lambda]"}], 
            RowBox[{"-", "1"}]},
           {
            RowBox[{"-", "1"}], 
            RowBox[{"2", "-", "\[Lambda]"}]}
          }], ")"}]}]}], "=", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"2", "-", "\[Lambda]"}], ")"}], 
         RowBox[{"(", 
          RowBox[{
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"2", "-", "\[Lambda]"}], ")"}], "2"], "-", "1"}], 
          ")"}]}], "-", 
        RowBox[{"\[ImaginaryI]", "(", 
         RowBox[{
          RowBox[{"-", 
           RowBox[{"\[ImaginaryI]", "(", 
            RowBox[{"2", "-", "\[Lambda]"}], ")"}]}], "-", "\[ImaginaryI]"}], 
         ")"}], "+", 
        RowBox[{"\[ImaginaryI]", " ", 
         RowBox[{"(", 
          RowBox[{
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"2", "-", "\[Lambda]"}], ")"}], "2"], "-", "1"}], 
          ")"}]}]}], "=", 
       RowBox[{"\[Ellipsis]", "=", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"-", "9"}], "\[Lambda]"}], "+", 
          RowBox[{"6", " ", 
           SuperscriptBox["\[Lambda]", "2"]}], "-", 
          SuperscriptBox["\[Lambda]", "3"]}], "=", 
         RowBox[{
          RowBox[{"-", 
           RowBox[{"\[Lambda]", "(", 
            RowBox[{
             SuperscriptBox["\[Lambda]", "2"], "-", 
             RowBox[{"6", "\[Lambda]"}], "+", "9"}], ")"}]}], "=", 
          RowBox[{
           RowBox[{"-", 
            RowBox[{"\[Lambda]", "(", 
             RowBox[{"\[Lambda]", "-", "3"}], ")"}]}], 
           RowBox[{"(", 
            RowBox[{"\[Lambda]", "-", "3"}], ")"}]}]}]}]}]}]}]}]}]}], 
  TraditionalForm]],ExpressionUUID->"1ec6692b-2514-4b71-990c-b4cc7e5e200f"]], \
"Text",
 CellChangeTimes->{{3.7867100547384396`*^9, 3.7867101928132086`*^9}, {
  3.7867102478615255`*^9, 3.786710257895734*^9}, {3.786713811391114*^9, 
