Cell[CellGroupData[{

Cell["Mathematics for 2D", "Section",
 CellChangeTimes->{{3.787915515500209*^9, 
  3.787915518540085*^9}},ExpressionUUID->"a85ea29d-a475-4b98-909c-\
73be48662d66"],

Cell[TextData[{
 "We got: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["v", "2"], "=", 
    RowBox[{
     SubsuperscriptBox["v", "2", "par"], "+", 
     SubsuperscriptBox["v", "2", "ort"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "87973c3f-2923-4946-8099-cc45b622dad8"],
 ", where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["u", "1"], "\[CenterDot]", 
      SubsuperscriptBox["v", "2", "ort"]}], ")"}], "=", "0"}], 
   TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "daaaabd7-66c7-4944-aaa1-48c97b555bc7"],
 ". Also ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["v", "2", "par"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "42c91b2e-4491-45a6-906c-ac4961a3353c"],
 " is parallel to ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["u", "1"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "1509eaf1-74ac-4726-a89c-42e7698f906e"],
 " and therefore it is a multiple of ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["u", "1"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "d3ed17cd-cec4-4a8f-91a7-21cfddf6ca8c"],
 " for some coefficient ",
 Cell[BoxData[
  FormBox["\[Alpha]", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "2d537905-7223-4871-a9a5-52afdb64ad46"],
 " in the following way: ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["v", "2", "par"], "=", 
    RowBox[{"\[Alpha]", "\[CenterDot]", 
     SubscriptBox["u", "1"]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "7b2b8758-79d4-4500-b609-3d7d9fc57088"],
 "."
}], "Text",
 CellChangeTimes->{{3.787913985754216*^9, 3.7879139874516826`*^9}, {
  3.7879141162364607`*^9, 3.787914223835824*^9}, {3.7879144268431854`*^9, 
  3.7879144298830614`*^9}, {3.7879155349721613`*^9, 3.7879156312308617`*^9}, {
  3.7879157412138815`*^9, 3.7879157511991863`*^9}},
 FontSize->36,ExpressionUUID->"37fe9425-0460-40ce-a7c6-b773abf6ed13"],

Cell[TextData[{
 "We can actually calculate the value of ",
 Cell[BoxData[
  FormBox["\[Alpha]", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "becf6389-bf0d-4ed0-97e7-d9005c582aae"],
 " by taking the scalar product of ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["v", "2"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "dd538aca-c23d-4669-95cd-01a00c7049d5"],
 " with ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["u", "1"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "cf870e29-fc9d-40ee-ac50-00e7bd7b2530"],
 " (also remember that ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["u", "1"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "5311c467-952b-49be-a444-c2a76b2e9af4"],
 " is a unit vector):"
}], "Text",
 CellChangeTimes->{{3.7879142030184684`*^9, 3.7879142507299747`*^9}, {
  3.787916499660555*^9, 3.787916545643979*^9}},
 FontSize->36,ExpressionUUID->"fec80a95-c260-4811-aa01-74338b83ddcd"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"(", 
    RowBox[{
     SubscriptBox["u", "1"], "\[CenterDot]", 
     SubscriptBox["v", "2"]}], ")"}], "=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["u", "1"], "\[CenterDot]", 
      RowBox[{"(", 
       RowBox[{
        SubsuperscriptBox["v", "2", "par"], "+", 
        SubsuperscriptBox["v", "2", "ort"]}], ")"}]}], ")"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["u", "1"], "\[CenterDot]", 
        SubsuperscriptBox["v", "2", "par"]}], ")"}], "+", 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["u", "1"], "\[CenterDot]", 
        SubsuperscriptBox["v", "2", "ort"]}], ")"}]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["u", "1"], "\[CenterDot]", 
         RowBox[{"(", 
          RowBox[{"\[Alpha]", "\[CenterDot]", 
           SubscriptBox["u", "1"]}], ")"}]}], ")"}], "+", "0"}], "=", 
      RowBox[{
       RowBox[{
        RowBox[{"\[Alpha]", "\[CenterDot]"}], "||", 
        SubscriptBox["u", "1"], 
        SuperscriptBox["||", "2"]}], "=", "\[Alpha]"}]}]}]}]}], 
  TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"ad1226ba-fde2-47a4-be17-9cc5520e4967"]], \
"Text",
 CellChangeTimes->{{3.787914209064311*^9, 3.787914296577386*^9}, {
  3.7879157654112005`*^9, 3.787915799772351*^9}, {3.787916550366321*^9, 
  3.7879165509587345`*^9}},
 FontSize->36,ExpressionUUID->"ee25d6df-abc4-4080-88fd-701dd902fa61"],

Cell["Putting all results together we obtain:", "Text",
 CellChangeTimes->{{3.7879158079494925`*^9, 3.7879158354390154`*^9}, {
  3.7879165565248566`*^9, 3.7879165682844596`*^9}},
 FontSize->36,ExpressionUUID->"7a945d33-a6b7-4b2a-aed8-9dfb049f8a05"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["v", "2"], "=", 
   RowBox[{
    RowBox[{
     SubsuperscriptBox["v", "2", "par"], "+", 
     SubsuperscriptBox["v", "2", "ort"]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"\[Alpha]", "\[CenterDot]", 
       SubscriptBox["u", "1"]}], "+", 
      SubsuperscriptBox["v", "2", "ort"]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         SubscriptBox["u", "1"], "\[CenterDot]", 
         SubscriptBox["v", "2"]}], ")"}], "\[CenterDot]", 
       SubscriptBox["u", "1"]}], "+", 
      SubsuperscriptBox["v", "2", "ort"]}]}]}]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"2f58c05e-6ac1-41bd-bce0-0eed57a70c4b"]], \
"Text",
 CellChangeTimes->{{3.7879158368363204`*^9, 3.7879158814550157`*^9}},
 FontSize->36,ExpressionUUID->"885bc2de-e1dc-415d-8e24-f902b33e5fdc"],

Cell[TextData[{
 "From this formula we can calculate ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["v", "2", "ort"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "eba054c6-18e6-46a7-b6d5-0def344c1f4a"],
 " as:"
}], "Text",
 CellChangeTimes->{{3.7879143191470594`*^9, 3.7879143437512903`*^9}, 
   3.7879144261131372`*^9, {3.7879158890357494`*^9, 3.787915904573247*^9}},
 FontSize->36,ExpressionUUID->"d0b467cd-b9ca-49a0-bad5-4bb901dc7f25"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SubsuperscriptBox["v", "2", "ort"], "=", 
   RowBox[{
    SubscriptBox["v", "2"], "-", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["u", "1"], "\[CenterDot]", 
       SubscriptBox["v", "2"]}], ")"}], "\[CenterDot]", 
     SubscriptBox["u", "1"]}]}]}], TraditionalForm]],
 FormatType->"TraditionalForm",
 FontWeight->
  "Bold",ExpressionUUID->"77652d69-d89f-41d1-bd2c-479376647aec"]], "Text",
 CellChangeTimes->{{3.7879158368363204`*^9, 3.7879158814550157`*^9}, {
  3.7879159189467974`*^9, 3.787915932364933*^9}, {3.787916577580577*^9, 
  3.787916578587881*^9}},
 FontSize->36,ExpressionUUID->"4463e217-986d-4b37-934f-e4f9ed1b0ccd"],

Cell[TextData[{
 "which is exactly the first formula of the Gram-Schmidtsches \
Orthogonalisierungsverfahren. To get ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["u", "2"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "fc577bb1-6285-4e51-9b80-9bb8a3cd20d0"],
 " we just normalize ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["v", "2", "ort"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "458a5fcb-dd9b-4bdd-b70c-e1e3b30c3dac"],
 ". "
}], "Text",
 CellChangeTimes->{{3.7879165807650614`*^9, 3.787916601138604*^9}, 
   3.787916663583727*^9, {3.7879167029734*^9, 3.787916718157812*^9}},
 FontSize->36,ExpressionUUID->"eb087ab9-bf16-442b-b292-cf23dfc66dfb"]
}, Closed]],

Cell[CellGroupData[{

Cell["3D", "Section",
 CellChangeTimes->{{3.786710629107031*^9, 3.786710630176859*^9}, {
  3.787910487607151*^9, 3.787910490520363*^9}, {3.7879123061385746`*^9, 
  3.7879123064088516`*^9}},ExpressionUUID->"a23834ac-fd5f-45e1-8490-\
bd0b0192b8ba"],

Cell[TextData[{
 "Similarly to the 2D case, we get the very first vector ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["u", "1"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "bf2b602f-a095-4de7-8711-620d7ad19ec5"],
 " by normalizing vector ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["v", "1"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "7fd31471-803f-4416-a989-1bec4cd038e6"],
 ". The second vector ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["v", "2"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "e627f7ba-3a04-4ab5-b6d6-aff1f63761f9"],
 " can then be written in the form:"
}], "Text",
 CellChangeTimes->{{3.7879165807650614`*^9, 3.787916601138604*^9}, 
   3.787916663583727*^9, {3.7879167029734*^9, 3.787916775868553*^9}, {
   3.7879168069085817`*^9, 3.7879168173895674`*^9}},
 FontSize->36,ExpressionUUID->"55bf7d65-d6a8-4ecd-8b18-c642bd18d92a"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["v", "2"], "=", 
   RowBox[{
    SubsuperscriptBox["v", "2", "par"], "+", 
    SubsuperscriptBox["v", "2", "ort"]}]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"8434c99c-ac45-4ec8-a8c6-b001e1e33d6c"]], \
"Text",
 CellChangeTimes->{{3.7879168182811823`*^9, 3.7879168184976034`*^9}},
 FontSize->36,ExpressionUUID->"69e65c13-b76b-4bfc-9fd2-2109b3353c6a"],

Cell["We proceed the same way as in the 2D case to get:", "Text",
 CellChangeTimes->{{3.7879168242203083`*^9, 3.7879168363648453`*^9}, {
  3.787918212814619*^9, 3.7879182130699368`*^9}},
 FontSize->36,ExpressionUUID->"5be4142f-2299-428f-99bf-08e532197535"],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["v", "2", "ort"], "=", 
    RowBox[{
     SubscriptBox["v", "2"], "-", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["u", "1"], "\[CenterDot]", 
        SubscriptBox["v", "2"]}], ")"}], "\[CenterDot]", 
      SubscriptBox["u", "1"]}]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",
  FontWeight->"Bold",ExpressionUUID->"2ef755d2-d789-4412-baba-cf8623a437f5"],
 "   and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["u", "2"], "=", 
    FractionBox[
     SubsuperscriptBox["v", "2", "ort"], 
     RowBox[{"||", 
      SubsuperscriptBox["v", "2", "ort"], "||"}]]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "5dd5b98b-afe4-40af-ad42-0158561812fd"]
}], "Text",
 CellChangeTimes->{{3.7879168409336324`*^9, 3.7879168633247814`*^9}},
 FontSize->36,ExpressionUUID->"acf3f9c5-b770-4614-85ef-bf0292f20e3a"],

Cell[TextData[{
 "The third vector can be decomposed into three parts now: one part is \
parallel to ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["u", "1"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "d4de88b9-08ba-47dd-ae97-e5d1ff41cbd2"],
 ", one part is parallel to ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["u", "2"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "61ba6973-2fd9-4894-b39f-0e5dba143faf"],
 ", and the last part is orthogonal to both ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["u", "1"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "d93c7aa5-9902-4d07-b96e-a7f3114becd6"],
 " and ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["u", "2"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "f22b00bc-83c5-4f11-be68-312111f21c41"],
 ":"
}], "Text",
 CellChangeTimes->{{3.7879168746525016`*^9, 3.7879169222044315`*^9}},
 FontSize->36,ExpressionUUID->"0bd4070c-05bf-4fef-b563-7376a9117b0c"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["v", "3"], "=", 
   RowBox[{
    RowBox[{"\[Alpha]", "\[CenterDot]", 
     SubscriptBox["u", "1"]}], "+", 
    RowBox[{"\[Beta]", "\[CenterDot]", 
     SubscriptBox["u", "2"]}], "+", 
    SubsuperscriptBox["v", "3", "ort"]}]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"95f71fe8-b5a8-44e7-a819-182c087bb8f1"]], \
"Text",
 CellChangeTimes->{{3.78791692316782*^9, 3.787916953502737*^9}},
 FontSize->36,ExpressionUUID->"218361f6-c7d4-416c-84e0-b62d25398a4d"],

Cell[TextData[{
 "As in the 2D case, we would calculate that ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Alpha]", "=", 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["u", "1"], "\[CenterDot]", 
      SubscriptBox["v", "3"]}], ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "e8edab9a-6966-4ebf-a815-4cd85df2d0eb"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"\[Beta]", "=", 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["u", "2"], "\[CenterDot]", 
      SubscriptBox["v", "3"]}], ")"}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "f5528838-3164-488e-af5f-6a14a31c0cbb"],
 ", so:"
}], "Text",
 InitializationCell->True,
 CellChangeTimes->{{3.7879169632456923`*^9, 3.787917029229352*^9}, {
  3.787918232910901*^9, 3.787918233183174*^9}},
 FontSize->36,ExpressionUUID->"fbdeddb8-3ab9-4c84-af7c-05a4878c771d"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["v", "3"], "=", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["u", "1"], "\[CenterDot]", 
       SubscriptBox["v", "3"]}], ")"}], "\[CenterDot]", 
     SubscriptBox["u", "1"]}], "+", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["u", "2"], "\[CenterDot]", 
       SubscriptBox["v", "3"]}], ")"}], "\[CenterDot]", 
     SubscriptBox["u", "2"]}], "+", 
    SubsuperscriptBox["v", "3", "ort"]}]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"828d181c-d4c0-4d98-beb5-7ed8ff4fcbbe"]], \
"Text",
 CellChangeTimes->{{3.78791692316782*^9, 3.787916953502737*^9}, {
  3.787917039712297*^9, 3.7879170452046556`*^9}},
 FontSize->36,ExpressionUUID->"c9b4f1d7-c673-4686-acdc-f330be4dcd5a"],

Cell[TextData[{
 "The last part ",
 Cell[BoxData[
  FormBox[
   SubsuperscriptBox["v", "3", "ort"], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "f8e5de3c-9a1a-4816-87c3-09a11d6033f4"],
 " is therefore equal to"
}], "Text",
 CellChangeTimes->{{3.7879170482863827`*^9, 3.7879170632603545`*^9}},
 FontSize->36,ExpressionUUID->"3e0eb699-258a-4c08-abdb-fe657c9a7ef0"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SubsuperscriptBox["v", "3", "ort"], "=", 
   RowBox[{
    SubscriptBox["v", "3"], "-", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["u", "1"], "\[CenterDot]", 
       SubscriptBox["v", "3"]}], ")"}], "\[CenterDot]", 
     SubscriptBox["u", "1"]}], "-", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       SubscriptBox["u", "2"], "\[CenterDot]", 
       SubscriptBox["v", "3"]}], ")"}], "\[CenterDot]", 
