Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"det", "(", 
    RowBox[{"D", "-", 
     RowBox[{"\[Lambda]", " ", "\[DoubleStruckCapitalI]\[DoubleStruckD]"}]}], 
    ")"}], "=", 
   RowBox[{
    RowBox[{"det", "(", GridBox[{
       {
        RowBox[{
         FractionBox["1", "2"], "-", "\[Lambda]"}], 
        FractionBox[
         SqrtBox["3"], "2"]},
       {
        FractionBox[
         SqrtBox["3"], "2"], 
        RowBox[{
         RowBox[{"-", 
          FractionBox["1", "2"]}], "-", "\[Lambda]"}]}
      }], ")"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         FractionBox["1", "2"], "-", "\[Lambda]"}], ")"}], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"-", 
          FractionBox["1", "2"]}], "-", "\[Lambda]"}], ")"}]}], "-", 
      SuperscriptBox[
       RowBox[{"(", 
        FractionBox[
         SqrtBox["3"], "2"], ")"}], "2"]}], "=", 
     RowBox[{"\[Ellipsis]", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"\[Lambda]", "-", "1"}], ")"}], 
       RowBox[{"(", 
        RowBox[{"\[Lambda]", "+", "1"}], ")"}]}]}]}]}]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"14bc8a27-afde-49dd-b01d-bee5f434a51b"]], \
"Text",
