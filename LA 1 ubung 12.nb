Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox["P", "2"], "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["v", "2"], 
     SubsuperscriptBox["v", "2", "T"]}], "=", 
    RowBox[{
     RowBox[{
      FractionBox["1", "2"], 
      RowBox[{
       RowBox[{"(", GridBox[{
          {
           RowBox[{"-", "1"}]},
          {
