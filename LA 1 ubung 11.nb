    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"1", "-", "\[Lambda]"}], ")"}], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"3", "-", "\[Lambda]"}], ")"}], 
         RowBox[{"(", 
          RowBox[{"-", "\[Lambda]"}], ")"}]}], "-", 
        RowBox[{"2", 
         RowBox[{"(", 
          RowBox[{"-", "1"}], ")"}]}]}], ")"}]}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "-", "\[Lambda]"}], ")"}], 
       RowBox[{"(", 
        RowBox[{"2", "+", 
         SuperscriptBox["\[Lambda]", "2"], "-", 
         RowBox[{"3", "\[Lambda]"}]}], ")"}]}], "=", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "-", "\[Lambda]"}], ")"}], 
        RowBox[{"(", 
         RowBox[{"\[Lambda]", "-", "1"}], ")"}], 
        RowBox[{"(", 
         RowBox[{"\[Lambda]", "-", "2"}], ")"}]}], "=", 
       RowBox[{
        RowBox[{"-", 
         SuperscriptBox[
          RowBox[{"(", 
