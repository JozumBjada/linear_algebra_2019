 CellChangeTimes->{{3.7867095175130625`*^9, 3.7867095377338295`*^9}},
 FontSize->36,
 FontWeight->"Bold",ExpressionUUID->"adf714df-4fa5-4749-8997-096a44731cb7"],

Cell[TextData[{
 "We will prove this relation by mathematical induction. The initial step has \
been done already, it suffices to do the induction step: we assume that the \
relation above is true for ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "e96e674d-dcdf-446a-af45-856ef17363da"],
 " and want to prove it for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "+", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "0679a34b-3e0f-4a27-b2a9-3e5345c33929"],
 ":"
}], "Text",
 CellChangeTimes->{{3.786709542675377*^9, 3.786709593048874*^9}},
 FontSize->36,ExpressionUUID->"6cfaee67-3d05-4d66-9316-727116b258d6"],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SuperscriptBox["A", 
    RowBox[{"n", "+", "1"}]], "=", 
   RowBox[{
    RowBox[{
     SuperscriptBox["A", "n"], "\[CenterDot]", "A"}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", GridBox[{
         {
          SuperscriptBox["\[Lambda]", "n"], 
          RowBox[{"n", " ", 
           SuperscriptBox["\[Lambda]", 
            RowBox[{"n", "-", "1"}]]}]},
         {"0", 
          SuperscriptBox["\[Lambda]", "n"]}
        }], ")"}], "\[CenterDot]", 
      RowBox[{"(", GridBox[{
         {"\[Lambda]", "1"},
         {"0", "\[Lambda]"}
        }], ")"}]}], "=", 
     RowBox[{
      RowBox[{"(", GridBox[{
         {
          RowBox[{
           SuperscriptBox["\[Lambda]", 
            RowBox[{"n", "+", "1"}]], "+", "0"}], 
          RowBox[{
           SuperscriptBox["\[Lambda]", "n"], "+", 
           RowBox[{"n", " ", 
            SuperscriptBox["\[Lambda]", "n"]}]}]},
         {
          RowBox[{"0", "+", "0"}], 
          SuperscriptBox["\[Lambda]", 
           RowBox[{"n", "+", "1"}]]}
        }], ")"}], "=", 
      RowBox[{"(", GridBox[{
         {
          SuperscriptBox["\[Lambda]", 
           RowBox[{"n", "+", "1"}]], 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"n", "+", "1"}], ")"}], 
           SuperscriptBox["\[Lambda]", "n"]}]},
         {"0", 
          SuperscriptBox["\[Lambda]", 
           RowBox[{"n", "+", "1"}]]}
        }], ")"}]}]}]}]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"cb7bc973-4beb-4ee2-8c5c-e10b29704345"]], \
"Text",
