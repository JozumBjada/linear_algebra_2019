Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"det", "(", 
    RowBox[{
     RowBox[{"A", "\[CenterDot]", "B"}], "-", 
     RowBox[{
     "t", "\[CenterDot]", "\[DoubleStruckCapitalI]\[DoubleStruckD]"}]}], 
    ")"}], "=", 
   RowBox[{
    RowBox[{"det", "(", 
     RowBox[{
      RowBox[{"S", "\[CenterDot]", 
       SubscriptBox["P", "r"], "\[CenterDot]", 
       SuperscriptBox["T", 
        RowBox[{"-", "1"}]], "\[CenterDot]", "B"}], "-", 
      RowBox[{
      "t", "\[CenterDot]", "\[DoubleStruckCapitalI]\[DoubleStruckD]"}]}], 
     ")"}], "=", 
    RowBox[{
     RowBox[{"det", "(", 
      RowBox[{"S", "\[CenterDot]", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          SubscriptBox["P", "r"], "\[CenterDot]", 
          SuperscriptBox["T", 
           RowBox[{"-", "1"}]], "\[CenterDot]", "B", "\[CenterDot]", "S"}], 
         "-", 
         RowBox[{
         "t", "\[CenterDot]", "\[DoubleStruckCapitalI]\[DoubleStruckD]"}]}], 
        ")"}], "\[CenterDot]", 
       SuperscriptBox["S", 
        RowBox[{"-", "1"}]]}], ")"}], "=", 
     RowBox[{
      RowBox[{
       RowBox[{"det", "(", "S", ")"}], "\[CenterDot]", 
       RowBox[{"det", "(", 
        RowBox[{
         RowBox[{
          SubscriptBox["P", "r"], "\[CenterDot]", 
          RowBox[{"(", 
           RowBox[{
            SuperscriptBox["T", 
             RowBox[{"-", "1"}]], "\[CenterDot]", "B", "\[CenterDot]", "S"}], 
           ")"}]}], "-", 
         RowBox[{
         "t", "\[CenterDot]", "\[DoubleStruckCapitalI]\[DoubleStruckD]"}]}], 
        ")"}], "\[CenterDot]", 
       RowBox[{"det", "(", 
        SuperscriptBox["S", 
         RowBox[{"-", "1"}]], ")"}]}], "=", 
      RowBox[{"det", "(", 
       RowBox[{
        RowBox[{
         SubscriptBox["P", "r"], "\[CenterDot]", 
         OverscriptBox["B", "_"]}], "-", 
        RowBox[{
        "t", "\[CenterDot]", "\[DoubleStruckCapitalI]\[DoubleStruckD]"}]}], 
       ")"}]}]}]}]}], TraditionalForm]],
