Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   SuperscriptBox["A", "n"], "=", 
   RowBox[{"(", GridBox[{
      {
       SuperscriptBox["\[Lambda]", "n"], 
       RowBox[{"n", " ", 
        SuperscriptBox["\[Lambda]", 
         RowBox[{"n", "-", "1"}]]}]},
      {"0", 
       SuperscriptBox["\[Lambda]", "n"]}
     }], ")"}]}], TraditionalForm]],
 FormatType->
  "TraditionalForm",ExpressionUUID->"167764b0-9aa6-41ca-be30-9d318875d08c"]], \
